{
  "name": "@wonse/eslint-react",
  "version": "2.1.0",
  "description": "ESLint configuration for React projects with modern best practices",
  "author": {
    "name": "Wonse Shin",
    "email": "sinwonse@gmail.com",
    "url": "https://github.com/shinwonse"
  },
  "homepage": "https://github.com/shinwonse/eslint-flat-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinwonse/eslint-flat-config.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/shinwonse/eslint-flat-config/issues"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup-node --minify",
    "dev": "tsup-node --watch",
    "clean": "rimraf .turbo node_modules dist"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@wonse/eslint-common": "workspace:*",
    "@wonse/eslint-ts": "workspace:*",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-hooks-extra": "^1.26.0",
    "eslint-plugin-react-web-api": "^1.26.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.5",
    "eslint": "^9.19.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
